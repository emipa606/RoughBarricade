<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="BuildingBase">
    <defName>RoughBarricade</defName>
    <label>Rough Barricade</label>
    <description>Sandbags, Cavalry Spikes, And Barbed Wire - A Wall With Clear Intentions - Untraversable.</description>
    <thingClass>Building</thingClass>
    <category>Building</category>
    <graphicData>
      <texPath>Things/Building/Security/RoughBarricade/RoughBarricade</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <color>(139,122,87)</color>
      <drawSize>2.0</drawSize>
    </graphicData>
    <uiIconPath>Things/Building/Security/RoughBarricade/RoughBarricade_UI</uiIconPath>
    <uiIconColor>(139,122,87)</uiIconColor>
    <altitudeLayer>Building</altitudeLayer>
    <pathCostIgnoreRepeat>true</pathCostIgnoreRepeat>
    <passability>Impassable</passability>
    <disableImpassableShotOverConfigError>true</disableImpassableShotOverConfigError>
    <castEdgeShadows>true</castEdgeShadows>
    <fillPercent>0.70</fillPercent>
    <canOverlapZones>false</canOverlapZones>
    <statBases>
      <MaxHitPoints>600</MaxHitPoints>
      <Beauty>-15</Beauty>
      <WorkToBuild>180</WorkToBuild>
      <Flammability>0</Flammability>
    </statBases>
    <size>(2,2)</size>
    <building>
      <ai_chillDestination>false</ai_chillDestination>
      <isInert>true</isInert>
    </building>
    <costList>
      <Steel>7</Steel>
      <WoodLog>2</WoodLog>
    </costList>
    <filthLeaving>SandbagRubble</filthLeaving>
    <rotatable>True</rotatable>
    <selectable>true</selectable>
    <neverMultiSelect>true</neverMultiSelect>
    <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
    <designationCategory>Security</designationCategory>
    <staticSunShadowHeight>0.20</staticSunShadowHeight>
    <constructEffect>ConstructDirt</constructEffect>
    <repairEffect>ConstructDirt</repairEffect>
    <designationHotKey>Misc1</designationHotKey>
  </ThingDef>
